    "examples": {
      "create_item": {
        "description": "Create a new item with all supported JSON data types",
        "method": "POST",
        "endpoint": "/items",
        "headers": {
          "Content-Type": "application/json"
        },
        "request_body": {
          "name": "Premium Wireless Headphones",
          "description": "High-quality wireless headphones with active noise cancellation and premium sound quality",
          "price": 299.99,
          "quantity": 50,
          "is_active": true,
          "tags": ["electronics", "audio", "wireless", "premium", "noise-cancelling"],
          "metadata": {
            "category": "electronics",
            "subcategory": "audio",
            "brand": "TechBrand",
            "model": "WH-1000XM5",
            "weight_grams": 250,
            "dimensions": {
              "length_mm": 254,
              "width_mm": 220,
              "height_mm": 102
            },
            "features": [
              "active-noise-cancellation",
              "bluetooth-5.2",
              "30-hour-battery",
              "quick-charge",
              "multipoint-connection"
            ],
            "specifications": {
              "driver_size_mm": 40,
              "frequency_response": "4Hz-40kHz",
              "impedance_ohms": 47,
              "sensitivity_db": 104.5,
              "wireless_range_meters": 10
            },
            "colors_available": ["black", "silver", "blue"],
            "warranty_months": 24,
            "certifications": ["FCC", "CE", "RoHS"],
            "sustainability": {
              "recyclable_packaging": true,
              "energy_efficient": true,
              "carbon_neutral_shipping": false
            }
          }
        },
        "expected_response": {
          "success": true,
          "data": {
            "id": "item-abc123def456",
            "name": "Premium Wireless Headphones",
            "description": "High-quality wireless headphones with active noise cancellation and premium sound quality",
            "price": 299.99,
            "quantity": 50,
            "is_active": true,
            "tags": ["electronics", "audio", "wireless", "premium", "noise-cancelling"],
            "metadata": {
              "category": "electronics",
              "brand": "TechBrand",
              "weight_grams": 250
            },
            "created_at": "2024-01-15T10:30:00Z",
            "updated_at": "2024-01-15T10:30:00Z"
          },
          "message": "Item 'item-abc123def456' created successfully",
          "timestamp": "2024-01-15T10:30:00Z"
        }
      },
