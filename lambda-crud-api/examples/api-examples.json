  "curl_commands": {
    "create_item": "curl -X POST https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items -H 'Content-Type: application/json' -d '{\"name\":\"Test Item\",\"price\":99.99,\"quantity\":10,\"is_active\":true}'",
    "get_all_items": "curl https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items",
    "get_item_by_id": "curl https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items/item-id",
    "update_item": "curl -X PUT https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items/item-id -H 'Content-Type: application/json' -d '{\"name\":\"Updated Item\",\"price\":149.99}'",
    "delete_item": "curl -X DELETE https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items/item-id",
    "filter_active_items": "curl 'https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items?is_active=true'",
    "filter_by_price_range": "curl 'https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items?min_price=50.00&max_price=200.00'",
    "filter_by_tag": "curl 'https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items?tag=electronics'",
    "combined_filters": "curl 'https://your-api-id.execute-api.ap-northeast-1.amazonaws.com/v1/items?is_active=true&min_price=100.00&tag=electronics'"
