    "error_examples": {
      "validation_error": {
        "description": "Example of validation error response",
        "method": "POST",
        "endpoint": "/items",
        "request_body": {
          "name": "",
          "price": -10.00,
          "quantity": "not-a-number",
          "is_active": "not-a-boolean",
          "tags": "not-an-array"
        },
        "expected_response": {
          "success": false,
          "error": {
            "code": "VALIDATION_ERROR",
            "message": "Validation failed",
            "details": [
              {
                "field": "name",
                "message": "Field 'name' must be at least 1 characters"
              },
              {
                "field": "price",
                "message": "Field 'price' must be at least 0.01"
              },
              {
                "field": "quantity",
                "message": "Field 'quantity' must be an integer"
              },
              {
                "field": "is_active",
                "message": "Field 'is_active' must be a boolean"
              },
              {
                "field": "tags",
                "message": "Field 'tags' must be an array"
              }
            ]
          },
          "timestamp": "2024-01-15T11:25:00Z"
        }
      },
